// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model User {
  id        String @id @map("_id") @db.ObjectId @default(auto())
  studentId String @unique
  name      String
  gate      Gate
}

enum Gate {
  AND
  OR
  NOR
  NOT
}
